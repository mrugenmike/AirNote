<!--End of header-->

<div class="container-default">
    <div class="row">
        <div class="col-md-4">
            <ul id="myTab" class="nav nav-tabs">
                <li class="active"><a href="#notes" data-toggle="tab" class="glyphicon glyphicon-book"> Notes</a></li>
                <li><a href="#reminder" data-toggle="tab" class="glyphicon glyphicon-calendar"> Reminder</a></li>
            </ul>

            <!--notes data-->
            <div id="myTabContent" class="tab-content">
                <div class="tab-pane fade in active" id="notes">
                    <p>
                    <div ng-repeat="noteDecorator in noteDecoratorList" ng-click="noteDecorator.setCurrentNote()"  >
                        <div class="panel panel-success" >
                            <div class="panel-heading" ng-click="$parent.editing = true;$parent.creating = false" >
                                <h3 class="panel-title" >
                                    <a class="noteTitle" >{{noteDecorator.note.title}}</a>
                                </h3>
                            </div>
                        </div>
                    </div>
                    <!-- prev next-->
                    <ul class="pager">
                        <li class="previous"><a href ="" ng-click="prevClicked();">&larr; Older</a></li>
                        <li class="next"><a href="#" ng-click="nextClicked();">Newer &rarr;</a></li>
                    </ul>
                    </p>
                </div>
                <div class="tab-pane fade" id="reminder">

                    <p>
                    <div class="panel panel-success" >
                        <div class="panel-heading" ng-click="$parent.editing = true;$parent.creating = false" >
                            <h3 class="panel-title" >
                                <a class="noteTitle" >Message1</a>
                            </h3>
                        </div>
                    </div>
                    </p>

                    <ul class="pager">
                        <li class="previous"><a href ="" ng-click="prevClicked();">&larr; Older</a></li>
                        <li class="next"><a href="#" ng-click="nextClicked();">Newer &rarr;</a></li>
                    </ul>

                </div>
            </div>
        </div>

        <div class="col-md-8">
            <!-- New note tab-->
            <ul class="nav navbar-nav">
                <form id = "createForm">

                    <li class="col-md-4" ng-hide="creating" ng-hide="creatingReminder">

                        <button class="btn btn-default" ng-click="creating = true; editing = false; creatingReminder=false" ng-disabled ="creatingReminder" style="margin-right : 100px"><b>
                                <span class="glyphicon glyphicon-pencil"></span> New Note</b></button>
                    </li>
                </form>
            </ul>

            <!-- New reminder tab-->
            <ul class="nav navbar-nav">
                <form id = "createForm">

                    <li class="col-md-4" ng-hide="creatingReminder" ng-hide="creating" >

                        <button class="btn btn-default" ng-click="creatingReminder = true; editingReminder = false; creating=false; editing = false" ng-disabled ="creating" style="margin-right : 100px"><b>
                                <span class="glyphicon glyphicon-bell"></span> Set new reminder</b></button>
                    </li>
                </form>
            </ul>

            <div class="clearfix"></div>

            <!-- Notes-->
                            <span ng-show="creating" >
                                 <div class="form-group">
                                     <label for="title">Title</label>
                                     <input name = "title" type="email" class="form-control" id="createTitle" ng-model="user.title" placeholder="Enter title">
                                 </div>
                                 <div class="form-group">
                                     <label for="exampleInputPassword1">Note</label>
                                     <textarea ng-model="newNoteContent" id ="createContent" class="form-control" rows="6"></textarea>
                                 </div>
                                    <button type="submit" class="btn btn-default" ng-click="create();">Save</button>

                                    <button type="submit" class="btn btn-default" ng-click="clear();reload(); creating=false" ng-click="creating=false" >Cancel</button>
                                    <label for="errorMessage" style="color: red; font-style: italic;"> {{ errorMessage }} </label>

                            </span>

                            <span ng-show="editing" >
                                 <div class="form-group">
                                     <label for="exampleInputEmail1">Title</label>
                                     <input type="email" class="form-control" id="updateTitle" value ="{{clickedNoteTitle}}">
                                 </div>
                                 <div class="form-group">
                                     <label for="exampleInputPassword1">Note</label>
                                     <textarea id="editContent" class="form-control" rows="6" value="{{currentNoteContent}}" ng-model="updateContent"></textarea>
                                 </div>
                                    <button type="submit" class="btn btn-default" ng-click="update();reload(); editing= false">Save</button>
                                <button type="submit" class="btn btn-default" ng-click="delete();reload(); editing=false" >
                                    <span class="glyphicon glyphicon-trash"></span> Delete</button>
                                    <button type="submit" class="btn btn-default" ng-click=" reload(); editing=false" >Cancel</button>
                            </span>
            <!-- Notes end-->

            <!--Reminder start -->

                             <span ng-show="creatingReminder" >

                                 <div class="form-group">
                                     
                                 </div>
                                 <div class="form-group">

                                 </div>

                                     <!--Content-->
                                     <div class="form-group">
                                         <p for="reminderContent">Note: </p>
                                         <textarea id="editContent" class="form-control" rows="6" value="{{currentReminder}}" ng-model="updateContent"></textarea>
                                     </div> 

                                    <button type="submit" class="btn btn-default" ng-click="create();">Save</button>

                                    <button type="submit" class="btn btn-default" ng-click="clear();reload(); creating=false" ng-click="creating=false" >Cancel</button>
                                    <label for="errorMessage" style="color: red; font-style: italic;"> {{ errorMessage }} </label>


                            </span>

                            <span ng-show="editingReminder" >
                                  <div class="form-group">
                                      <h1></h1>
                                      <p>Date: <input type="text" id="datepicker"></p>
                                  </div>
                                 <div class="form-group">
                                     <div class="input-append bootstrap-timepicker">
                                         <p> Time:
                                             <input id="timepicker1" type="text" class="input-small">
                                             <span class="add-on"><i class="icon-time"></i></span>
                                         </p>
                                     </div>

                                     <script type="text/javascript">
                                         $('#timepicker1').timepicker();
                                     </script>
                                 </div>

                                     <!--Content-->
                                     <div class="form-group">
                                         <p for="reminderContent">Note: </p>
                                         <textarea id="editContent" class="form-control" rows="6" value="{{currentReminder}}" ng-model="updateContent"></textarea>
                                     </div> 

                                    <button type="submit" class="btn btn-default" ng-click="create();">Save</button>

                                    <button type="submit" class="btn btn-default" ng-click="clear();reload(); creating=false" ng-click="creating=false" >Cancel</button>
                                    <label for="errorMessage" style="color: red; font-style: italic;"> {{ errorMessage }} </label>

                            <!--Reminder end-->

        </div>
    </div>

</div>

